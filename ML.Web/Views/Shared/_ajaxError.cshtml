<div id="jsError" class="modal fade" role="dialog">
    <div class="modal-dialog jsError">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" style="margin-top:-10px">&times;</button>
            </div>
            <div class="modal-body">
                <div class="th-model-header">
                    <span class="error-label">Error ID:</span> : <span id="spnErrorID"></span> (Error Number : <span id="spnErrorNumber"></span>)
                </div>
                <hr class="th-hr" />
                <div>
                    <span class="error-label">Error Message</span> : <span id="divErrorMessage"></span>
                </div>
                <hr class="th-hr" />
                <div>
                    <span class="error-label">Error Trace</span> : <span id="divErrorTrace"></span>
                </div>
            </div>
        </div>
    </div>
</div>

